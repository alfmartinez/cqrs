{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-classes-per-file": [
            false
        ],
        "only-arrow-functions": [
            true,
            "allow-declarations",
            "allow-named-functions"
        ],
        "no-console": [
            true,
            "log",
            "error"
        ]
    },
    "rulesDirectory": []
}